{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/segurbot-widget.umd.cjs",
      "dest": "/segurbot-widget.umd.cjs",
      "headers": {
        "Content-Type": "application/javascript",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/segurbot-widget.css",
      "dest": "/segurbot-widget.css",
      "headers": {
        "Content-Type": "text/css",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
